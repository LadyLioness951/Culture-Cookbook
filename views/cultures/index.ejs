<%- include('../partials/header') %>

<nav>
  <% if (user) { %>
  <a href="/cultures/new">Add Culture</a>
  <% } %>
</nav>
<div class="wrapper-grid">
  <% cultures.forEach(function(c) { %>
    <a href="/cultures/<%= c._id %>">
      <div class="container">
        <h1><%= c.name %></h1>
        <img class="image" src="<%= c.image %>">
      </div>
    </a>
  <% }); %>
</div>

<%- include('../partials/footer') %>
